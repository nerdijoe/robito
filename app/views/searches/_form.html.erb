  <div class="form-group">
    <%= form.label :keyword, 'Keyword', class:'col-sm-2 control-label' %>
    <div class="col-sm-3">
      <%= form.text_field :keyword , class: 'form-control'%>
    </div>
  </div>

  <div class="form-group">
    <%= form.label :location, 'Location', class:'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= form.select :location, Influencer.locations.map{ |a| [a.first,a.second] }, include_blank: true , class: 'form-control'%>
    </div>
  </div>

  <div class="form-group">
    <%= form.label :age, 'Audience Age', class:'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= form.select :age, Influencer.ages.map{ |a| [a.first,a.second] }, include_blank: true , class: 'form-control'%>
    </div>
  </div>

  <div class="form-group">
    <%= form.label :gender, 'Target Gender', class:'col-sm-2 control-label' %>
    <div class="col-sm-10">
      <%= form.select :gender, Influencer.genders.map{ |a| [a.first,a.second] }, include_blank: true,  class: 'form-control'%>
    </div>
  </div>

  <div class="form-group">
    <%= form.label :interests, 'Interests', class:'col-sm-2 control-label' %>
    <div class="col-sm-3">
      <%= form.text_field :interests , class: 'form-control'%>
    </div>
  </div>
