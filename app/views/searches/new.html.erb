<div class="container">

  <h2>Search Influencers</h2>

  <%= form_for @search, html: {class: 'form-horizontal'} do |form| %>

  <%= render partial: '/searches/form', object: form %>

    <div class="form-group">
      <div class="col-sm-offset-2 col-sm-10">
