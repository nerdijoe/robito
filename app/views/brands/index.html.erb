<h2>Brand#Index</h2>
<p>
  <%= link_to "Signout", destroy_user_session_path, method: :delete %>
</p>


<% @campaigns.each do |c| %>
  <p>c.id</p>
  <p>c.product_name</p>
  <p>c.description</p>
  <p>-------</p>
<div class="campaign">
            <% c.image.each do |photo| %>
               <%= image_tag photo.url(:thumb) %>
            <% end %>
               
      </div>
<% end %>
