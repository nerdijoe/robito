<div class="form-group">
  <%= form.label :brand_name, 'Brand Name', class:'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= form.text_field :brand_name %>
  </div>
</div>

<div class="form-group">
  <%= form.label :description, 'Description', class:'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= form.text_field :description %>
  </div>
</div>
 
<div class="form-group">
	<%= form.label :company_name, 'Company Name', class:'col-sm-2 control-label' %>
    <div class="col-sm-10">
    <%= form.text_field :company_name %>
  </div>
 </div>

<div class="form-group">    
  <%= form.label :email, 'Email', class:'col-sm-2 control-label' %>    
    <div class="col-sm-10">      
    <%= form.text_field :email %>    
  </div>  
</div>

  <div class="form-group">    
    <%= form.label :phone_number, 'Phone Number', class:'col-sm-2 control-label' %>    
      <div class="col-sm-10">      
      <%= form.text_field :phone_number %>    
    </div>  
  </div>

  <div class="form-group">    
    <%= form.label :address, 'Address', class:'col-sm-2 control-label' %>    
      <div class="col-sm-10">      
      <%= form.text_field :address %>    
    </div>  
  </div>

  <div class="form-group">    
    <%= form.label :link, 'Link', class:'col-sm-2 control-label' %>    
      <div class="col-sm-10">      
      <%= form.text_field :link %>    
    </div>  
  </div>

<div class="form-group">
    <div class="col-sm-8">      
    <%= form.file_field :image, multiple: true %>
  </div>
</div>
