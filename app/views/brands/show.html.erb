<h2>Brand Profile</h2>

<p><%= @brand.user.id %></p>
<p><%= @brand.user.email %></p>
<p><%= @brand.user.type %></p>
<p>
  <% if @brand.user.identities.present? %>
    <% @brand.user.identities.each do |i| %>
      <p><%= i.provider %></p>
      <p><%= i.nickname %></p>
    <% end %>
  <% end %>
</p>
